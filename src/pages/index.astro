---
import MainLayout from "../layouts/MainLayout.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
import Hero from "../components/Hero.astro";
import CardSlider from "../components/CardSlider.astro";
import Some from "../components/Some.astro";
import Bidrag from "../components/Bidrag.astro";
import Brev from "../components/Brev.astro";
import Shop from "../components/Shop.astro";
import Mobilbutton from '../components/Mobilbutton.astro';
import Button from '../components/Button.astro';
import Link from '../components/Link.astro';
import { Image } from 'astro:assets';
import '../styles/tailwind.css';
import 'animate.css';
const { title } = Astro.props;

---
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>{Astro.props.title}</title>
	</head>
	<header>
		<Header></Header>
	</header>
	<body>
			<Hero></Hero>
			<CardSlider></CardSlider>
			<Bidrag></Bidrag>
			<Some></Some>
			<Brev></Brev>
			<Shop></Shop>

	</body>
	<footer>
		<Footer></Footer>
	</footer>
</html>
